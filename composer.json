{
    "name": "firelike/nytimes-api",
    "description": "Zend Framework module to consume NY Times APIs",
    "type": "library",
    "require": {
        "php": "^5.6",
        "guzzlehttp/guzzle": "^6.2",
        "zendframework/zend-servicemanager": "^3.1",
        "zendframework/zend-mvc-console": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Firelike\\NYTimes\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Firelike\\NYTimes\\Test\\": "tests\/"
        }
    }
}
