{
    "name": "firelike/nytimes-api",
    "description": "Zend Framework module to consume NY Times APIs",
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "php": "^8.4.6",
        "laminas/laminas-servicemanager": "^3.1",
        "guzzlehttp/guzzle-services": "^1.0",
        "laminas/laminas-http": "^2.14.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Firelike\\NYTimes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Firelike\\NYTimes\\Test\\": "tests/"
        }
    }
}
